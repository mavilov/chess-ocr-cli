{
  "name": "chess-ocr-cli",
  "version": "1.0.0",
  "description": "A CLI tool to convert chess game protocols from images to PGN format using AI vision",
  "main": "dist/index.js",
  "bin": {
    "chess-ocr-cli": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "ts-node src/index.ts",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "keywords": [
    "chess",
    "ocr",
    "cli",
    "pgn",
    "ai",
    "vision"
  ],
  "author": "Maksim Avilov",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^22.15.21",
    "commander": "^14.0.0",
    "dotenv": "^16.5.0",
    "openai": "^5.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "prettier": "^3.5.3"
  }
}
